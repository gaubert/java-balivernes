<?xml version="1.0" encoding="utf-8"?>

<SampML xmlns="http://www.ctbto.org/SAMPML/0.5">
	<Remark>${REMARK}</Remark>
	<SampleInformation UUID="${SAMPLEID}">
		<StationInformation>
			<StationLocation>${STATION_LOCATION}</StationLocation>
			<StationCode>${STATION_CODE}</StationCode>
			<CountryCode>${COUNTRY_CODE}</CountryCode>
			<Coordinates>${COORDINATES}</Coordinates>  
		</StationInformation>
		<DetectorInformation>
			<DetectorCode>${DET_CODE}</DetectorCode>
			<DetectorDescription>${DET_DESCRIPTION}</DetectorDescription>
		</DetectorInformation>
		<SampleType>${SAMPLE_TYPE}</SampleType>
		<SampleGeometry>${SAMPLE_GEOMETRY}</SampleGeometry>
		<SampleQuantity unit="m3">${SAMPLE_QUANTITY}</SampleQuantity>
		<MeasuredInformation>
            ${SPECTRUM}
		</MeasuredInformation>
		<Analyses>
		   ${AnalysisResults}
		</Analyses>
		<CalibrationInformation>
			${CALIBRATION}
		</CalibrationInformation>
	</SampleInformation>
</SampML>