[GroupTest1]
astring=oracle.jdbc.driver.OracleDriver
# some comments
aint= 10
afloat=5.24
abool1  = true
abool2  = false
abool3  = yes
abool4  = no
abool5  = NO
aPath   = /tmp/foo/bar
fromenv = /mydir/%(ENV[TESTENV])
fromcli1 = %(CLI[--LongName]) is embedded
fromcli2 = %(CLI[LongName]) is embedded 2


[GroupTestVars]
path   = /foo/bar/%(GroupTest1[aPath])/bar/foo
