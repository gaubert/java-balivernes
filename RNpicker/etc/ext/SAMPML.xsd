  <xs:complexType name="peakResultsType">
    <xs:annotation>
      <xs:documentation>List of Peaks Results</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="PeakResult">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>Peak detail</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="Energy" type="doubleUnc">
              <xs:annotation>
                <xs:documentation>
									Pair of energy and energy
