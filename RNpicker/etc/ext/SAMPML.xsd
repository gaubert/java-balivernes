<?xml version="1.0" encoding="utf-8"?>

<xs:schema xmlns="http://www.ctbto.org/SAMPML/0.5" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.ctbto.org/SAMPML/0.5" id="sampml" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:annotation>
    <xs:documentation>
			SAMPML IDC Format for describing RN Sample information
			Version: 0.5
		</xs:documentation>
  </xs:annotation>
  <xs:element name="SampML">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" name="Remark" type="xs:string">
          <xs:annotation>
            <xs:documentation>
				Description (i.e comments) related to this sample
		    </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="1" maxOccurs="unbounded" name="SampleInformation" type="sampleInformationType" />
      </xs:sequence>
    </xs:complexType>
  </xs:element> 
  <xs:complexType name="sampleInformationType">
    <xs:annotation>
      <xs:documentation>
				   All informations related to one sample
			     </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="StationInformation" type="stationInformationType">
        <xs:annotation>
          <xs:documentation>
						Time, location information for the sample
					   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="DetectorInformation" type="detectorInformationType">
        <xs:annotation>
          <xs:documentation>
						Information related to the detector used in the
						station (geometry, ?)
					   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="SampleType" type="enumSampleType">
        <xs:annotation>
          <xs:documentation>
					 </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="SampleGeometry" type="xs:string">
        <xs:annotation>
          <xs:documentation>
						Only for particulate
					 </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="SampleQuantity" type="physicalValueType" />
      <xs:element minOccurs="1" maxOccurs="1" name="MeasuredInformation" type="measuredInformationType">
        <xs:annotation>
          <xs:documentation>
						scientifically measured data
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="AnalysisResults" type="analysisResultsType">
        <xs:annotation>
          <xs:documentation>
						Analysis data (identified nuclides)
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Parameters" type="parametersType">
        <xs:annotation>
          <xs:documentation>
						Processing, Updating parameters used for doing
						the analysis
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Flags" type="flagsType">
        <xs:annotation>
          <xs:documentation>
						The different flags used
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="CalibrationInformation" type="calibrationInformationType">
        <xs:annotation>
          <xs:documentation>
						calibration equations
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xs:sequence>
    <xs:attribute name="UUID" type="xs:string">
      <xs:annotation>
        <xs:documentation>
					Unique identifier for this measurement
				</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="stationInformationType">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="StationLocation" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="StationCode" type="xs:string">
        <xs:annotation>
          <xs:documentation>
						It should be part of an enumerate containing all
						station codes (gazeteer)
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="CountryCode" type="xs:string">
        <xs:annotation>
          <xs:documentation>
						It should be part of an enumerate containing all
						country codes (gazeteer)
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Coordinates" type="coordinatesType">
        <xs:annotation>
          <xs:documentation>
						Geographical coordinates (latitude, longitude,
						elevation)
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="StartTime" type="xs:dateTime" />
      <xs:element minOccurs="0" maxOccurs="1" name="EndTime" type="xs:dateTime" />
      <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##other" processContents="lax" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="detectorInformationType">
    <xs:annotation>
      <xs:documentation>To Be Defined</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DetectorCode" type="xs:string" />
      <xs:element name="DetectorDescription" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="measuredInformationType">
    <xs:annotation>
      <xs:documentation>
				Sampled information sent by the station
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="Spectrum" type="spectrumType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="spectrumType">
    <xs:annotation>
      <xs:documentation>
				Sampled information sent by the station
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="CollectionStart" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
						Measurement start date/time
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="CollectionStop" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
						Measurement stop date/time
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="AcquisitionStart" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
						Acquition start date/time
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="AcquisitionStop" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
						Acquition stop date/time
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="RealAcquisitionTime" type="xs:duration">
        <xs:annotation>
          <xs:documentation>
						Duration of each sample measurement/interval at
						which samples are taken. The Real Acquisition time is the time
						when the detector is really active
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="LiveAcquisitionTime" type="xs:duration">
        <xs:annotation>
          <xs:documentation>
						Duration of each sample measurement/interval at
						which samples are taken. Total time when the AcqStop - AcqStart
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="SamplingTime" type="xs:duration">
        <xs:annotation>
          <xs:documentation>
						Effective duration (i.e., live time) of each
						sample measurement
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="DecayTime" type="xs:duration">
        <xs:annotation>
          <xs:documentation>
						Effective duration (i.e., live time) of each
						sample measurement
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="SourceType" type="enumSourceType">
        <xs:annotation>
          <xs:documentation>
						Source kind: background, Sample, ...
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Data" type="channelDataType">
        <xs:annotation>
          <xs:documentation>
						the data, spectrum
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="calibrationIDs" type="stringListType">
      <xs:annotation>
        <xs:documentation>
					Unique identifier(s) for this measurement
				</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:group name="particulateAnalysisResultsType">
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>
					Analysis Results for particulates
				</xs:documentation>
      </xs:annotation>
      <xs:element minOccurs="1" maxOccurs="1" name="SampleCategorization" type="sampleCategorizationType">
        <xs:annotation>
          <xs:documentation>
						Categorization of the sample
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="ActivityConcentrationSummary" type="activityConcentrationSummaryType" />
      <xs:element minOccurs="1" maxOccurs="1" name="PeakResults" type="peakResultsType" >
       <xs:annotation>
          <xs:documentation>
						peaks information
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="0" name="SCACLines"   type="scacLinesType">
       <xs:annotation>
          <xs:documentation>
			SCAC lines information, only for autoSAINT results.
		  </xs:documentation>
        </xs:annotation>
      </xs:element>
       
    </xs:sequence>
  </xs:group>
  
  <xs:complexType name="scacLinesType">                                            <!-- added for autoSaint -->
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="0" name="Data"   type="channelDataType"/>
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  
  
  <xs:complexType name="analysisResultsType">
    <xs:annotation>
      <xs:documentation>
				Categorization, Activity Summary, ...
			</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:group ref="particulateAnalysisResultsType" />
      <xs:group ref="nobleGasAnalysisResultsType" />
    </xs:choice>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:group name="nobleGasAnalysisResultsType">
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>
					Analysis results for noble gas
				</xs:documentation>
      </xs:annotation>
      <xs:element minOccurs="1" maxOccurs="1" name="ActivityConcentrationSummary" type="activityConcentrationSummaryType" />
      <xs:element minOccurs="1" maxOccurs="1" name="ActivitySummary">
        <xs:annotation>
          <xs:documentation>Sample activity decay corrected</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Nuclide" type="nuclideType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="ROINetCountResults" type="ROINetCountResultsType" />
      <xs:element minOccurs="1" maxOccurs="1" name="XeCovarianceMatrix" type="XeCovarianceMatrixType" />
    </xs:sequence>
  </xs:group>
  <xs:complexType name="XeCovarianceMatrixType">
    <xs:annotation>
      <xs:documentation>
				Intermediate results, ROI Net Count results
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="GoodnessOfFit"  type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="Rows"           type="stringListType" />
      <xs:element minOccurs="1" maxOccurs="1" name="Cols"           type="stringListType" />
      <xs:element minOccurs="1" maxOccurs="unbounded" name="Values" type="doubleListType" />
    </xs:sequence>
    <xs:attribute name="method" type="xs:string" />
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="ROIBoundariesType">
    <xs:annotation>
      <xs:documentation>
				Intermediate results, ROI Net Count results
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="ROIBoundary">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="Nb" type="xs:positiveInteger" />
            <xs:element minOccurs="1" maxOccurs="1" name="GammaLow" type="physicalValueType" />
            <xs:element minOccurs="1" maxOccurs="1" name="GammaHigh" type="physicalValueType" />
            <xs:element minOccurs="1" maxOccurs="1" name="BetaLow" type="physicalValueType" />
            <xs:element minOccurs="1" maxOccurs="1" name="BetaHigh" type="physicalValueType" />
          </xs:sequence>
          <xs:anyAttribute namespace="##other" processContents="lax" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="method" type="xs:string" />
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="ROINetCountResultsType">
    <xs:annotation>
      <xs:documentation>
				Intermediate results, ROI Net Count results
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="ROI">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="Nb" type="xs:positiveInteger" />
            <xs:element minOccurs="1" maxOccurs="1" name="Nuclide" type="xs:string" />
            <xs:element minOccurs="1" maxOccurs="1" name="NetCounts" type="xs:integer" />
            <xs:element minOccurs="1" maxOccurs="1" name="NetCountsError" type="physicalValueType" />
            <xs:element minOccurs="1" maxOccurs="1" name="LC" type="physicalValueType" />
            <xs:element minOccurs="1" maxOccurs="1" name="Efficiency" type="physicalValueType" />
          </xs:sequence>
          <xs:anyAttribute namespace="##other" processContents="lax" />
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="ROIUsedFor">
        <xs:annotation>
          <xs:documentation>
						Describe which ROI have been used for identifying a
						nuclide
					</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ROIs" type="nonNegativeIntegerList" />
          </xs:sequence>
          <xs:attribute name="nuclide" type="xs:string" />
          <xs:anyAttribute namespace="##other" processContents="lax" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="method" type="xs:string" />
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:complexType name="activityConcentrationSummaryType">
    <xs:annotation>
      <xs:documentation>
				Summary of the sample activity
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>
					Activity Concentration results for particulates and noble gas
				</xs:documentation>
      </xs:annotation>
      <xs:element minOccurs="0" maxOccurs="1" name="NonQuantifiedNuclides">
        <xs:annotation>
          <xs:documentation>
						Non quantified identified nuclides
					</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Nuclide" type="nuclideType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="QuantifiedNuclides">
        <xs:annotation>
          <xs:documentation>
						Quantified identified nuclides
					</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Nuclide" type="nuclideType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="MinimumDetectableConcentration">
        <xs:annotation>
          <xs:documentation>
						Categorization of the sample
					</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Nuclide" type="nuclideType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="nuclideType">
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>
					Nuclide Type. Contains all the activity information
					regarding a specific nuclide
				</xs:documentation>
      </xs:annotation>
      <xs:element minOccurs="1" maxOccurs="1" name="Name" type="xs:string">
        <xs:annotation>
          <xs:documentation>Nuclide Name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Type" type="enumNuclideType">
        <xs:annotation>
          <xs:documentation>
						Natural, Activation, Fission
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="HalfLife" type="xs:duration">
        <xs:annotation>
          <xs:documentation>
						Half-life as a string
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Concentration" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>Concentration</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ConcentrationError" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>
						Concentration error
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="MinMDA" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>Min MDA</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="MDA" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>MDA</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="MaxMDA" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>Max MDA</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="Activity" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>Activity in Bq</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ActivityError" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>
						Activity error in Bq
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ActivityCriticalLimit" type="physicalValueType">
        <xs:annotation>
          <xs:documentation>
						Activity critical limit in Bq
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="NIDFlag" type="xs:integer">
        <xs:annotation>
          <xs:documentation>NID-Flag</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:anyAttribute namespace="##other" processContents="lax" />
  </xs:complexType>
  <xs:group name="procParam_Genie_RRR_v1.0Type">
    <xs:annotation>
      <xs:documentation>
				Different groups qualifiying the processing parameters
				Group Names are Software_Method_Version Particulate Old
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Threshold" type="xs:positiveInteger" />
      <xs:element name="PeakStart" type="physicalValueType" />
      <xs:element name="PeakEnd" type="physicalValueType" />
      <xs:element name="LeftFWHMLimit" type="xs:positiveInteger" />
      <xs:element name="RightFWHMLimit" type="xs:positiveInteger" />
      <xs:element name="MutlipletFWHMLimit" type="xs:positiveInteger" />
      <xs:element name="FitSinglet" type="xs:boolean" />
      <xs:element name="CriticalLevelTest" type="xs:boolean" />
      <xs:element name="EstimatePeakWidths" type="xs:boolean" />
      <xs:element name="BaselineType" type="xs:string" />
      <xs:element name="BaselineChannels" type="xs:positiveInteger" />
      <xs:element name="PerformSubstraction" type="xs:boolean" />
      <xs:element name="EnergyTolerance" type="xs:double" />
      <xs:element name="ConfidenceThreshold" type="xs:double" />
      <xs:element name="RiskLevel" type="physicalValueType" />
    </xs:sequence>
  </xs:group>
  <xs:group name="procParam_Autosaint_None_v1.0Type">
    <xs:sequence>
      <xs:element name="toBeCompleted" type="xs:string" />
    </xs:sequence>
  </xs:group>
  <xs:group name="procParam_BGAnalyze_SSM_v1.0Type">
    <xs:sequence>
      <xs:element name="UseDetectorBackground" type="xs:boolean" />
      <xs:element name="UseGasBackground" type="xs:boolean" />
      <xs:element name="LCAbissa" type="xs:double" />
      <xs:element name="BinningGammaOffset" type="xs:positiveInteger" />
      <xs:element name="BinningBetaOffset" type="xs:positiveInteger" />
      <xs:element name="BinningMinimumCounts" type="xs:positiveInteger" />
      <xs:element name="BinningNbMergedRows" type="xs:positiveInteger" />
      <xs:element name="BinningVectorSize" type="xs:positiveInteger" />
    </xs:sequence>
  </xs:group>
  <xs:group name="procParam_BGAnalyze_NCC_v1.0Type">             <!--  (bg_analyze?)-->
    <xs:sequence>
       <xs:element minOccurs="1" maxOccurs="1" name="ROIBoundaries"      type="ROIBoundariesType" />
    </xs:sequence>
  </xs:group>
  <xs:group name="updateParam_Genie_RRR_v1.0Type">
    <xs:annotation>
      <xs:documentation>
				For old software and SPALAX, extracted from
				GARDS_SAMPLE_PROC_PARAMS
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="UseMRP" type="xs:boolean" />
      <xs:element minOccurs="0" name="MRPSampleID" type="xs:positiveInteger" />
      <!--  it is optional for the moment -->
      <xs:element name="MDAConfidence" type="xs:double" minOccurs="0"/>
      <xs:element name="GainShift" type="physicalValueType" />
      <xs:element name="ZeroShift" type="xs:double" />
      <xs:element name="AreaLimit" type="xs:positiveInteger" />
      <xs:element name="UseWeight" type="xs:boolean" />
      <xs:element name="UseMultiplet" type="xs:boolean" />
      <xs:element name="ForceLinear" type="xs:boolean" />
      <xs:element name="IgnorePreviousECR" type="xs:boolean" />
      <xs:element name="MinimumLibLookupTolerance" type="xs:double" />
      <xs:element name="RERIntercept" type="xs:double" />
      <xs:element name="RERSlope" type="xs:double" />
      <xs:element name="ECRSlope" type="xs:double" />
      <xs:element name="DoResolutionUpdate" type="xs:boolean" />
      <xs:element minOccurs="0" maxOccurs="1" name="RERMRPUsed" type="xs:positiveInteger" />
    </xs:sequence>
  </xs:group>
  <xs:group name="updateParam_Autosaint_RRR_v1.0Type">
    <xs:annotation>
      <xs:documentation>
				To be changed. For the moment it is the same.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ToBeDefined" type="xs:boolean" />
    </xs:sequence>
